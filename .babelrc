{
  "presets": [
    ["@babel/preset-env", {
      "useBuiltIns": "usage"
    }],
    "@babel/preset-react"
  ],
  "plugins": ["@babel/plugin-proposal-class-properties"],
  "env": {
    "production": {
      "plugins": ["transform-remove-console"]
    },
    "development": {
      "presets": [
        ["@babel/preset-react", {
          "development": true
        }]
      ]
    }
  }
}
